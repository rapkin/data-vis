{
  "name": "data-vis",
  "version": "1.0.0",
  "description": "Data visualisation with python backend",
  "main": "main.py",
  "scripts": {
    "python-deps": "pip install -r requirements.txt",
    "install": "npm run python-deps && npm run build",
    "init-db": "createdb data-vis && createuser -w 'data-vis' && python init_db.py",
    "start": "python main.py",
    "lint": "eslint ./source/**/*.js ./source/**/*.jsx",
    "clean": "rimraf dist",
    "build-frontend": "cross-env NODE_ENV=production webpack -p",
    "build-frontend-dev": "cross-env NODE_ENV=development webpack -w",
    "build": "npm run clean && npm run build-frontend",
    "build-dev": "npm run clean && npm run build-frontend-dev",
    "dev": "npm run build-dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rapkin/data-vis.git"
  },
  "author": "rapkin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rapkin/data-vis/issues"
  },
  "homepage": "https://github.com/rapkin/data-vis#readme",
  "dependencies": {
    "axios": "^0.16.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "classnames": "^2.2.5",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.2",
    "eslint": "^3.19.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-react": "^7.0.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "font-awesome": "^4.7.0",
    "history": "^4.6.1",
    "json-loader": "^0.5.4",
    "leaflet": "^1.0.3",
    "lodash": "^4.17.4",
    "node-sass": "^4.5.3",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-hot-loader": "^1.3.1",
    "react-leaflet": "^1.2.0",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "^5.0.0-alpha.6",
    "redux": "^3.6.0",
    "redux-form": "^6.7.0",
    "redux-promise-middleware": "^4.3.0",
    "redux-thunk": "^2.2.0",
    "resolve-url-loader": "^2.0.2",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.18.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.6.0",
    "webpack-dev-server": "^2.4.5"
  }
}
